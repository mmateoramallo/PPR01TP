al) ifTrue: [costoPaquete := costoPaquete * 0.1]	ifFalse:[costoPaquete := costoPaquete * porcentajeExtra].	! !!CircuitoNacional methodsFor: 'accessing' stamp: 'Mateo 4/24/2023 15:18'!costoPaquete	^ costoPaquete! !!CircuitoNacional methodsFor: 'accessing' stamp: 'Mateo 4/24/2023 15:18'!costoPaquete: anObject	costoPaquete := anObject! !!CircuitoNacional methodsFor: 'accessing' stamp: 'Mateo 4/24/2023 15:18'!enFechaEspecial: anObject	enFechaEspecial := anObject! !!CircuitoNacional methodsFor: 'accessing' stamp: 'Mateo 4/24/2023 15:18'!enFechaEspecial	^ enFechaEspecial! !!CircuitoNacional methodsFor: 'accessing' stamp: 'Mateo 4/24/2023 15:18'!porcentajeExtra	^ porcentajeExtra! !!CircuitoNacional methodsFor: 'accessing' stamp: 'Mateo 4/24/2023 15:18'!porcentajeExtra: anObject	porcentajeExtra := anObject! !PaqueteTuristico subclass: #PaqueteMedida	instanceVariableNames: 'visa itinerario costoServicios costoTransporte costoExcusiones dias'	classVariableNames: ''	package: 'TrabajoPractico01'!!PaqueteMedida methodsFor: 'propios' stamp: 'Mateo 4/24/2023 16:12'!calcularPrecio	"Sumo todos los servicios y lo multiplico por la cantidad de d√≠as definido por el cliente"	|costoFinal|	costoFinal := ((costoServicios + costoTransporte + costoExcusiones) * dias) + visa costo .	^ costoFinal! !!PaqueteMedida methodsFor: 'propios' stamp: 'Mateo 4/24/2023 16:06'!asignarVisa: unaVisa	visa := unaVisa.! !!PaqueteMedida methodsFor: 'initialization' stamp: 'Mateo 4/25/2023 15:47'!initialize	super initialize.	visa := Visa new.	itinerario := ''.	costoServicios := 0.	costoTransporte := 0.	costoExcusiones := 0.	dias := 0.! !!PaqueteMedida methodsFor: 'converting' stamp: 'Mateo 4/24/2023 16:22'!asString	^ super asString, String cr,	'Visa:', visa asString, String cr,	'Itinerario:', itinerario, String cr,	'Costo Final:', costoGestion calcularPrecio asString, '$',String cr.! !!PaqueteMedida methodsFor: 'accessing' stamp: 'Mateo 4/24/202