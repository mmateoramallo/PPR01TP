: 'Mateo 4/22/2023 20:52'!costo: anObject	costo := anObject! !!Visa methodsFor: 'accessing' stamp: 'Mateo 4/22/2023 20:52'!tipo: anObject	tipo := anObject! !!Visa methodsFor: 'propios' stamp: 'Mateo 4/22/2023 21:24'!costoFinalVisa| costoDesc |    (tipo = 1 or: [tipo = 2]) ifTrue: [        costoDesc := costo * 0.95.    ] ifFalse: [        costoDesc := costo.    ].    ^ costoDesc.! !